<structure name="pipeline">
    <structure name="input">
        <field name="channelId" pattern="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}|[0-9a-fA-F]{32}" type="java.util.UUID"/>
    </structure>
    <structure name="output"/>
    <structure collectionName="channelTransfers" definition="nabu.frameworks.io.database.types.channelInstanceData" maxOccurs="unbounded" minOccurs="0" name="transfers"/>
    <structure collectionName="channels" definition="nabu.frameworks.io.database.types.channel" name="channel"/>
</structure>
